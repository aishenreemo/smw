# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 4
end_of_line = lf
trim_trailing_whitespace = true

[*.js]
indent_style = space
indent_size = 4

[*.html]
indent_style = space
indent_size = 4

[*.css]
indent_style = space
indent_size = 4

[*.json]
indent_style = space
indent_size = 2
